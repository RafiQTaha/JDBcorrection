<div class="modal" tabindex="-1" role="dialog" id="generer-modal" aria-modal="true"> 
<div class="modal-dialog modal-md" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Generer une sessions</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"> 
        <span aria-hidden="true">Ã—</span>
        </button>
      </div>
      <div class="modal-body">
        <!-- <div class="alert alert-info">Please check your browser console</div> -->
        <form id="save" method="post">
            <label for="designation">Designation:</label>
            <input type="text" name="designation" id="designation" class="form-control mb-2">
            <label for="designation">Enseignant:</label>
            <select  class="form-control" name="enseignant" id="enseignant">
              <option value="">Choix Enseignant </option>
              {% for enseignant in enseignants %}
              <option value="{{enseignant.id}}">{{enseignant.nom}}  {{enseignant.prenom }}</option>
              {% endfor %}
            </select>
            
          <button class="btn btn-success mt-2" type="submit" id="fire-modal-1-submit"><i class="fas fa-check-circle"></i> Enregistre</button>
        </form>
      </div>
    </div>
  </div>
</div>